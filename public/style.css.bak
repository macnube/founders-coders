span, li, p, h1 {
	font-family: Helvetica;
}

.main-container {
	position: relative;
	left: 50%;
	margin-left: -400px;
	width: 800px;
}

.header-container {
	width: 100%;
	height: 50px;
	margin-top: 25px;
	background-color: transparent;
	border-color: black;
	border-bottom-style: solid;
	border-bottom-width: 5px;
}

.logo-container {
	display: inline-block;
	height: 50px;
	width: 50px;
}

.logo {
	height: 100%;
	object-fit: contain;
}

.nav-container {
	float: right;
	height: 50px;
	vertical-align: middle;
	align-items: center;

}

.nav-button {
	line-height: 50px
}

.content-container {
	margin-top: 50px;
	height: 100%;
	width: 100%;
}

.about-container {
	height: 500px;
	width: 100%;
}

.profile-container {
	display: inline-block;
	height: 100%;
}

.profile-image {
	height: 100%;
	object-fit: contain;
}

.about {
	position: relative;
	float: right;
	width: 50%;
	height: 100%;
}

.about-text {
	position: relative
}

.about-text p {
}

.name {
	font-weight: bold;
	font-size: 80px;
	position: relative;
	left: 0px;
	top: 10px;
}

.read-more {
	position: absolute;
	bottom: 0px;
	font-size: 20px;
	right: 0px;
}

.atdaa-container {
	width: 100%;
	height: 500px;
	margin-top: 20px;
}

.atdaa-text {
	float: right;
	width: 450px;
	height: 100%;
}

.atdaa-image-container {
	display: inline-block;
	width: 300px;
}

.atdaa-image {
	width: 100%;
	object-fit: contain;
}